<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="odashboard_api_endpoint" model="ir.config_parameter">
            <field name="key">odashboard.api.endpoint</field>
            <field name="value">https://odashboard.app</field>
        </record>

        <record id="odashboard_connection_url" model="ir.config_parameter">
            <field name="key">odashboard.connection.url</field>
            <field name="value">https://app.odashboard.app</field>
        </record>
        
        <record id="param_odashboard_github_base_url" model="ir.config_parameter">
            <field name="key">odashboard.github_base_url</field>
            <field name="value">https://raw.githubusercontent.com/Notdoo/odashboard.engine/main/</field>
        </record>
    </data>
</odoo>
